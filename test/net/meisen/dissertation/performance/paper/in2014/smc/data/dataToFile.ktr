    <connection>Tida_Gh_Tasks</connection>
    <sql>SELECT 
  I.DIM_DEG_TASKOID                  AS KEY
, P.PERSONNR 						 AS PERSON
, T.TYPE							 AS TASKTYPE
, L.DEPARTMENT || &apos;.&apos; || L.WORKAREA	 AS WORKAREA
, I.INTERVAL_START
, I.INTERVAL_END
FROM
  TB_FACTS_TASK_INTERVAL I
, (SELECT KEY_PERSON, TO_CHAR(KEY_PERSON, &apos;0000000&apos;) AS PERSONNR, FISRTNAME AS FIRSTNAME, LASTNAME FROM TB_DIM_PERSON) P
, (SELECT KEY_TASKTYPE, DECODE(DURATION, &apos;kurze Task (&lt;= 30)&apos;, &apos;short&apos;, &apos;sehr lange Task (&gt; 120)&apos;, &apos;very long&apos;, &apos;lange Task (&gt; 60 &amp; &lt;= 120)&apos;, &apos;long&apos;, &apos;normale Task (&gt; 30 &amp; &lt;= 60)&apos;, &apos;average&apos;, &apos;unknown&apos;) AS TYPE FROM TB_DIM_TASKTYPE) T
, (SELECT KEY_LOCATION, &apos;W&apos; || TRIM(TO_CHAR(KEY_LOCATION,&apos;00&apos;)) AS WORKAREA, DECODE(DEPARTMENT, &apos;BD&apos;, &apos;MEI&apos;, &apos;GD&apos;, &apos;SEN&apos;, &apos;PB&apos;, &apos;EDI&apos;, &apos;VF&apos;, &apos;SON&apos;, &apos;VF2&apos;, &apos;DEB&apos;, &apos;VL&apos;, &apos;BIE&apos;, &apos;UNK&apos;) AS DEPARTMENT FROM TB_DIM_LOCATION) L
WHERE
     I.DIM_KEY_LOCATION = L.KEY_LOCATION
 AND I.DIM_KEY_PERSON   = P.KEY_PERSON
 AND I.DIM_KEY_TASKTYPE = T.KEY_TASKTYPE</sql>
