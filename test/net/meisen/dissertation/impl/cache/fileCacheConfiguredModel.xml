<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<model xmlns="http://dev.meisen.net/xsd/dissertation/model"
       xmlns:file="http://dev.meisen.net/xsd/dissertation/cache/file"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://dev.meisen.net/xsd/dissertation/model http://dev.meisen.net/xsd/dissertation/tidaModel.xsd
                           http://dev.meisen.net/xsd/dissertation/cache/file http://dev.meisen.net/xsd/dissertation/tidaFileCache.xsd"
       
       id="fileCacheConfiguredModel">
  
  <config>
    <cache implementation="net.meisen.dissertation.impl.cache.FileCache">
      <file:config folder="${java.io.tmpdir}/tmpFileCacheModelTest">
        <file:strategy timeThresholdFactor="5.25" weightingTime="0.2" />
      </file:config>
    </cache>
  </config>
  
  <data />
</model>