<?xml version="1.0" encoding="UTF-8" ?>

<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    
  <!-- IntegerNaturals with Minute -->
  <bean id="IntegerNaturalsMinute" class="net.meisen.dissertation.models.impl.time.RawTemporalModel">
    <constructor-arg type="net.meisen.dissertation.models.INaturalsFactory">
      <bean class="net.meisen.dissertation.models.impl.naturals.IntegerNaturalsFactory" />
    </constructor-arg>
    <constructor-arg type="net.meisen.dissertation.models.IRawTimeGranularity">
      <bean class="net.meisen.dissertation.models.impl.granularity.rawtime.Minute" />
    </constructor-arg>
  </bean>
</beans>