<?xml version="1.0" encoding="UTF-8" ?>

<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
  
  <!-- set the properties to be used -->
  <bean class="net.meisen.general.sbconfigurator.config.placeholder.SpringPropertyHolder">
    <property name="systemPropertiesModeName" value="SYSTEM_PROPERTIES_MODE_NEVER" />
    <property name="localOverride" value="false" />
    <property name="otherHolderOverride" value="false" />
    <property name="properties">
      <value>
        tida.config.selector=tidaConfig.xml
        tida.server.http.port=7070
	    tida.server.http.enabled=true
	    tida.server.tsql.port=7000
	    tida.server.tsql.enabled=true
	    tida.server.tsql.timeout=1800000
	    tida.server.control.port=7001
	    tida.server.control.enabled=false
      </value>
    </property>
  </bean>
</beans>